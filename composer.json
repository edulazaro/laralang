{
    "name": "edulazaro/laralang",
    "description": "Laravel localization package",
    "license": "MIT",
    "type": "library",
    "require": {
        "php": "^8.2",
        "laravel/framework": ">=10.0"
    },
    "autoload": {
        "psr-4": {
            "EduLazaro\\Laralang\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "EduLazaro\\Laralang\\Tests\\": "tests/"
        }
    },
    "authors": [
        {
            "name": "Edu Lazaro",
            "email": "edu@edulazaro.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "EduLazaro\\Laralang\\LaralangServiceProvider"
            ]
        }
    },
    "require-dev": {
        "phpunit/phpunit": "^10.5",
        "orchestra/testbench": "^8.34"
    }
}
